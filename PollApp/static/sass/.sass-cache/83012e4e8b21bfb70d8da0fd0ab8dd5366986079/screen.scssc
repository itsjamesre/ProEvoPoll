	font-weight:normal;	font-style:normal}





/* ===== Variables ===== */

$blueprint-grid-columns: 4;
$blueprint-grid-width: 256px;
$blueprint-grid-margin: 0px;
$blueprint-grid-outer-width: $blueprint-grid-width + $blueprint-grid-margin;
$blueprint-container-size: $blueprint-grid-outer-width * $blueprint-grid-columns - $blueprint-grid-margin;

$color0: #fff; // default color
$color1: #666;
$color2: #E2277E;

$color0-lite: $color0 + #555;

$color2-lite: $color2 + #555;
$color2-dark: $color2 - #111111;

$default-font: Arial,sans-serif;
$alt-font: "ExpresswayRegular",Arial,sans-serif;
$alt-font-lite: "ExpresswayLight",Arial,sans-serif;
$alt-font-bold: "ExpresswayBold",Arial,sans-serif;





/* ===== Mixins ===== */

@mixin input_base {
	background: url(../img/input-bg.png) 0 0 repeat-x #fff;
	border: 1px solid #c3c3c3;
	@include border-radius;
	@include box-shadow(rgba(250,250,250,0.75) 0 0 5px, #ccc 0 0 5px inset);
	color: #999;
	font-family: $default-font;
	font-size: 1.2em;
	height: 32px;
	line-height: 32px;
	min-width: 38px;
	padding: 0 10px;
	text-align: left;
	vertical-align: top;
}

@mixin li-checkbox {
	li {
		background: url(../img/check-blue.png) 0 0 no-repeat;
		font-size: 1.15em;
		margin-bottom: 0.75em;
		padding-left: 35px;
	}
}

@mixin button_base {
	@include border-radius;
	background: url(../img/select-bg.png) 0 bottom repeat-x #fff;
	border: 1px solid #bbb;
	color: #888;
	cursor: pointer;
	display: inline-block;
	font-family: $alt-font;
	font-size: 1em;
	letter-spacing: 0.1em;
	padding: 2px 5px;
	text-decoration: none;
	text-shadow: 1px 1px 1px #fff;
	vertical-align: top;
}

@mixin box_base {
	@include border-radius(5px);
	@include box-shadow(0 0 10px rgba(0,0,0,0.06));
	background: #fff;
	border: 1px solid #eee;
	margin: 5px 0;
	padding: 20px;
}





/* ===== Global Typographic / Element Styles ===== */

a {
   
   &:link, &:visited {
      color: $color2;
   }
   
   &:hover, &:active {
      color: $color2-dark;
   }
}

a.tag {
   background: #eee;
   border: 1px solid #ccc;
   border-radius: 5px;
   color: $color1;
   display: inline-block;
   font-size: 0.9em;
   line-height: 1em;
   text-decoration: none;
   padding: 3px 6px;
}

a.more {
   color: $color0;
   font-size: 0.9em;
   margin: 0 2px;
   text-decoration: none;
   text-transform: uppercase;
   
   &:hover, &:active {
      color: #000;
      text-decoration: underline;
   }
}

h1 {
   font-family: 'SommetSlabBlack', Arial, sans-serif;
   font-size: 2.5em;
   font-weight: normal;
   letter-spacing: 0.03em;
   line-height: 0.95em;
   text-align: center;
   text-transform: uppercase;
   
   text-shadow: 0 0 4px rgba(0, 0, 0, 0.9), 0 0 10px rgba(255, 255, 255, 0.2);
}

h2 {
   font-family: 'Asap', Arial, sans-serif;
   font-size: 1.6em;
   font-weight: 400;
   line-height: 1.3em;
   margin: 0 0 15px;
   
   em {
      color: $color2;
      font-weight: 400;
   }
}

h3 {
   font-family: 'Asap', Arial, sans-serif;
   font-size: 1.1em;
   font-weight: 500;
   line-height: 1.2em;
   margin: 0.5em 0;
}

h4 {
   font-family: 'Asap', Arial, sans-serif;
   font-size: .85em;
   font-weight: 700;
   line-height: 1.2em;
   margin: 0.25em 0;
}

p, ul, ol {
   font-family: 'Asap', Arial, sans-serif;
   font-size: .85em;
   font-weight: 400;
   line-height: 1.5em;
   margin: 0 0 0.5em;
   
   strong {
      font-weight: 700;
   }
   
   em {
   
   }
}

ul, ol {
   margin: 0;
   padding: 0;
}

fieldset {
   border-top: 1px solid #666;
   color: #ccc;
   margin-bottom: 5px;
   padding: 10px 0;
}

label {
   // color: #ccc;
   display: block;
   line-height: 1.4em;
   // padding-right: 150px;
   
   strong {
      color: #fff;
   }
}

input[type="radio"] {
   margin-left: 5px;
   
   &:first-child {
      margin-left: 0;
   }
}

button.nextStep, input[type="submit"] {
   background: $color2;
   border: 1px solid #ccc;
   border-collapse: collapse;
   @include border-radius(5px);
   color: #fff;
   font-size: 1em;
   font-weight: 700;
   padding: 8px 14px;
   
   box-shadow:
      0 0 5px rgba(0, 0, 0, 0.9), 
      0 0 20px rgba(255, 255, 255, 0.1), 
      inset 0 -18px 0 rgba(0, 0, 0, 0.2), 
      inset -5px -5px 5px rgba(0, 0, 0, 0.5), 
      inset 5px 5px 5px rgba(255, 255, 255, 0.2);
   
   &:hover, &:active {
      background: $color2-dark;
   }

   &.start-button {
      font-size: 2em;
      padding: 12px 24px;
      @include border-radius(10px);
      box-shadow:
         0 0 5px rgba(0, 0, 0, 0.9), 
         0 0 20px rgba(255, 255, 255, 0.1), 
         inset 0 -30px 0 rgba(0, 0, 0, 0.2), 
         inset -5px -5px 5px rgba(0, 0, 0, 0.5), 
         inset 5px 5px 5px rgba(255, 255, 255, 0.2);
   }
}

.none { display: none !important; }

// global text-shadow for some elements

h2, label, footer, input[type="submit"] {
   text-shadow: 0 0 2px rgba(0, 0, 0, 1.0), 0 0 10px rgba(0, 0, 0, 0.5);
}

/* ===== jQuery Mobile ui overides ===== */

.ui-btn-inner, .ui-loader { display: none; }




/* ===== Layout Styles & Specific Element Style Overrides ===== */

body {
   background: url('../img/wood2.png') 0 0 repeat #fff;
   color: $color0;
   font-size: 18px;
   line-height: 1.45em;
}

header {
   // @include column(1);
   float: left;
   width: 150px;
   padding-top: 20px;
}
   
section {
   // @include column(3, last);
   
   float: left;
   margin-bottom: 5px;
   // width: 768px;
   width: 618px;
   @include clearfix;
}

footer {
   // @include column(3, last);
   color: $color2;
   clear: both;
   float: left;
   font-size: 11px;
   letter-spacing: 0.06em;
   padding-left: 150px;
}
#wrapper {
   overflow: hidden;
   #polls {
      width: 100%;
      position: absolute;
      -webkit-backface-visibility: hidden;
      -webkit-transform: translate3d(0,0,0);
      padding-bottom: 20px;
      .poll {
         width: 100%;
         -webkit-transform: translate3d(0,0,0);
         .splash {
            text-align: center;
            .start-bar {
               @include border-radius(10px);
               height: 150px;
               .col { 
                  width: 100%;  
                  &:last-child { float:right; } 
                  h2 { margin-top: 20px; }
                  .start-button { margin-top: 40px; }
               }
            }
         }
         article {
            background: rgba(0,0,0,.5);
            padding: 40px 50px;
            @include border-radius(18px);
            margin: 20px;
            /* box-shadow performs miserably with transform3d in webkit */
            /*box-shadow:
               0 0 0px rgba(0,0,0,0.9),
               0 0 50px rgba(255, 255, 255, .1),
               inset 0 0 50px rgba(0, 0, 0, 0.9);;*/
            &.splash { background: transparent;}
            &.ratingStep {
               fieldset {
                  background: rgba(0,0,0,.25);
                  @include border-radius(10px);
                  padding: 20px;
                  margin-bottom: 20px;
                  h2 {}
                  label {
                     width: 620px;
                     font-size: 1.1em;
                  }
               }
               .ratingbuttons {
                  float: right;
               }
            }
            &.multiStep {
               fieldset {
                  background: rgba(0,0,0,.25);
                  @include border-radius(10px);
                  padding: 15px 20px;
                  margin-bottom: 20px;
                  span {
                     padding-left: 20px;
                     font-size: 1.1em;
                  }
               }
            }
            .radio {
               background: url('../img/shiny-btn50-off.png') 0 0 no-repeat transparent;
               color: #333;
               curser: pointer;
               display: inline-block;
               font-weight: 700;
               height: 50px;
               line-height: 50px;
               text-align: center;
               text-decoration: none;
               width: 50px;
               
               &.on {
                  background: url('../img/shiny-btn50-on.png') 0 0 no-repeat transparent;
                  color: #000;
               }
               
               &:hover, &:active {
                  color: #000;
               }
            }

            .multi-choice {
               font-size: .95em;
               a.radio {
                  margin-right: 10px;
               }
            }

            textarea { width: 70%; }

            button.nextStep {
               position: relative;
               bottom: 0px;
            }

            .results {
               float: left;
               width: 48%;
               padding-right: 20px;
               &:last-child { float: right; padding-right: 0px; }
               strong {
                  padding: 0px 3px;
                  color: white;
                  border-right: solid 1px white;
                  padding-right: 10px;
               }
               .labels {
                  position: absolute;
                  top:175px;
                  left: 0px;
                  strong { border: none; }
               }
               span { display: block; }
               p { clear: both; }
               & .multiple {
                  padding-right: 0px;
                  .xAxis {
                     height: 20px;
                  }
                  .answers_col { width: 50%; float: left;}
               }
               h3 {
                  text-align: center;
                  padding-bottom: 5px;
                  &:first-child { border-bottom: solid 1px white; font-weight: bold; }
               }
               .ratings {
                  .rate_bar {
                     @include border-radius(10px);
                     border: solid 1px rgb(92,92,92);
                     overflow: hidden;
                     height: 30px;
                     .rate_bar_value {
                        height: 35px;
                        background-image: -webkit-gradient(
                           linear,
                           left bottom,
                           left top,
                           color-stop(0.09, rgb(92,92,92)),
                           color-stop(1, rgb(255,255,255))
                        );
                     }
                     span {
                        float: right;
                        color: #e2277e;
                        font-weight:700;
                        padding-right: 5px;
                        padding-top: 5px;
                     }
                  }
                  strong {
                     padding: 0px;
                     border: none;
                  }
               }
            }

            &.resultsStep {
               display: none;
               .contact_info {
                  float: left;
                  width: 30%;
                  padding-right: 20px;
                  &.email {
                     padding-right: 0px;
                  }
               }
               input[type=text], input[type=email] { color: #999; }
               .saveResults {
                  clear: both;
               }
               .thankyou {
                  text-align: center;
               }
               .opt_in { display: block; font-size: .85em;}
               .opt_out { margin-top: 20px; }
               .submitRaffle { clear: both; }
            }
         }
         &:last-child .resultsStep { display: block; }
      }
   }
}

#overlay {
   display: none;
   position: absolute;
   top: 0px;
   left: 0px;
   width: 100%;
   height: 100%;
   background: rgba(0,0,0,.8);
   section {
      width: 100%;
      padding-top: 220px;
      article {
         width: 100%;
         margin-top: 50px;
         text-align: center;
         a {
            font-size: 4em;
            text-decoration: none;
            font-weight: bold;
            &.cancel {
               font-size: 2em;
            }
         }  
      }
   }
}



