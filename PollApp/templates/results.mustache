        <div class="results">
            <fieldset class="ratings">

                <h3>Rating {{ title }}</h3>

                <span class="{{rating_label_1}}">
                    <div class="rate_bar"><span><p>{{question1_avg}}/5</p></span><div class="rate_bar_value"></div></div>
                    <strong class="none">{{question1_avg}}</strong> <h3>{{rating_label_1}}</h3>
                </span>

                <span class="{{rating_label_2}}">
                    <div class="rate_bar"><span><p>{{question2_avg}}/5</p></span><div class="rate_bar_value"></div></div>
                    <strong class="none">{{question2_avg}}</strong> <h3>{{rating_label_2}}</h3>
                </span>

                <span class="{{rating_label_3}}">
                    <div class="rate_bar"><span><p>{{question3_avg}}/5</p></span><div class="rate_bar_value"></div></div>
                    <strong class="none">{{question3_avg}}</strong> <h3>{{rating_label_3}}</h3>
                </span>

                <span class="{{rating_label_4}}">
                    <div class="rate_bar"><span><p>{{question4_avg}}/5</p></span><div class="rate_bar_value"></div></div>
                    <strong class="none">{{question4_avg}}</strong> <h3>{{rating_label_4}}</h3>
                </span>

            </fieldset>

            <fieldset class="multiple">
                <h3>Changing {{ title }}</h3>

                <div class="pollForm" id="plotMultiple-{{ id }}" style="height:200px;">{{ id }}</div>

                <span><strong>A</strong> {{ multiple_answer_1 }}</span>
                <span><strong>B</strong> {{ multiple_answer_2 }}</span>
                <span><strong>C</strong> {{ multiple_answer_3 }}</span>
                <span><strong>D</strong> {{ multiple_answer_4 }}</span>
                <span><strong>E</strong> {{ multiple_answer_5 }}</span>
            </fieldset>
        </div>