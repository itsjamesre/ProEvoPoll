/* Custom Stylesheet for PE Website iteration - started March 2012 */





/* ===== Compass Imports ===== */

@import "blueprint";
// @import "blueprint/reset"; // this may not be necessary
@import "compass/utilities/general/clearfix";
@import "compass/css3/font-face";
@import "compass/css3/border-radius";
@import "compass/css3/box-shadow";
@import "compass/css3/inline-block";
@import "compass/css3/images";
@import "compass/support";
@import "partials/base";

// To generate css equivalent to the blueprint css but with your
// configuration applied, uncomment:
// @include blueprint

// If you are doing a lot of stylesheet concatenation, it is suggested
// that you scope your blueprint styles, so that you can better control
// what pages use blueprint when stylesheets are concatenated together.
body {
  /* @include blueprint-typography(true); */
  @include blueprint-utilities;
  @include blueprint-debug;
  @include blueprint-interaction;
  // Remove the scaffolding when you're ready to start doing visual design.
  // Or leave it in if you're happy with how blueprint looks out-of-the-box
}





/* ===== Web Fonts ===== */

@font-face {font-family:'SommetSlabBold';
	src:url('../fonts/SommetSlabBold-webfont.eot');
	src:local('‚ò∫'), 
		url('../fonts/SommetSlabBold-webfont.woff') format('woff'), 
		url('../fonts/SommetSlabBold-webfont.ttf') format('truetype'), 
		url('../fonts/SommetSlabBold-webfont.svg#webfont') format('svg');
	font-weight:normal; font-style:normal}

@font-face {font-family:'SommetSlabBoldItalic';
	src:url('../fonts/SommetSlabBoldItalic-webfont.eot');
	src:local('‚ò∫'), 
		url('../fonts/SommetSlabBoldItalic-webfont.woff') format('woff'), 
		url('../fonts/SommetSlabBoldItalic-webfont.ttf') format('truetype'), 
		url('../fonts/SommetSlabBoldItalic-webfont.svg#webfont') format('svg');
	font-weight:normal; font-style:normal}

@font-face {font-family:'SommetSlabBlack';
	src:url('../fonts/SommetSlabBlack-webfont.eot');
	src:local('‚ò∫'), 
		url('../fonts/SommetSlabBlack-webfont.woff') format('woff'), 
		url('../fonts/SommetSlabBlack-webfont.ttf') format('truetype'), 
		url('../fonts/SommetSlabBlack-webfont.svg#webfont') format('svg');
	font-weight:normal; font-style:normal}

@font-face {font-family:'SommetSlabBlackItalic';
	src:url('../fonts/SommetSlabBlackItalic-webfont.eot');
	src:local('‚ò∫'), 
		url('../fonts/SommetSlabBlackItalic-webfont.woff') format('woff'), 
		url('../fonts/SommetSlabBlackItalic-webfont.ttf') format('truetype'), 
		url('../fonts/SommetSlabBlackItalic-webfont.svg#webfont') format('svg');
	font-weight:normal; font-style:normal}

@font-face {font-family:'SommetSlabHeavy';
	src:url('../fonts/SommetSlabHeavy-webfont.eot');
	src:local('‚ò∫'), 
		url('../fonts/SommetSlabHeavy-webfont.woff') format('woff'), 
		url('../fonts/SommetSlabHeavy-webfont.ttf') format('truetype'), 
		url('../fonts/SommetSlabHeavy-webfont.svg#webfont') format('svg');
	font-weight:normal; font-style:normal}

@font-face {font-family:'SommetSlabHeavyItalic';
	src:url('../fonts/SommetSlabHeavyItalic-webfont.eot');
	src:local('‚ò∫'), 
		url('../fonts/SommetSlabHeavyItalic-webfont.woff') format('woff'), 
		url('../fonts/SommetSlabHeavyItalic-webfont.ttf') format('truetype'), 
		url('../fonts/SommetSlabHeavyItalic-webfont.svg#webfont') format('svg');
	font-weight:normal;	font-style:normal}





/* ===== Variables ===== */

$blueprint-grid-columns: 4;
$blueprint-grid-width: 256px;
$blueprint-grid-margin: 0px;
$blueprint-grid-outer-width: $blueprint-grid-width + $blueprint-grid-margin;
$blueprint-container-size: $blueprint-grid-outer-width * $blueprint-grid-columns - $blueprint-grid-margin;

$color0: #fff; // default color
$color1: #666;
$color2: #E2277E;

$color0-lite: $color0 + #555;

$color2-lite: $color2 + #555;
$color2-dark: $color2 - #111111;

$default-font: Arial,sans-serif;
$alt-font: "ExpresswayRegular",Arial,sans-serif;
$alt-font-lite: "ExpresswayLight",Arial,sans-serif;
$alt-font-bold: "ExpresswayBold",Arial,sans-serif;





/* ===== Mixins ===== */

@mixin input_base {
	background: url(../img/input-bg.png) 0 0 repeat-x #fff;
	border: 1px solid #c3c3c3;
	@include border-radius;
	@include box-shadow(rgba(250,250,250,0.75) 0 0 5px, #ccc 0 0 5px inset);
	color: #999;
	font-family: $default-font;
	font-size: 1.2em;
	height: 32px;
	line-height: 32px;
	min-width: 38px;
	padding: 0 10px;
	text-align: left;
	vertical-align: top;
}

@mixin li-checkbox {
	li {
		background: url(../img/check-blue.png) 0 0 no-repeat;
		font-size: 1.15em;
		margin-bottom: 0.75em;
		padding-left: 35px;
	}
}

@mixin button_base {
	@include border-radius;
	background: url(../img/select-bg.png) 0 bottom repeat-x #fff;
	border: 1px solid #bbb;
	color: #888;
	cursor: pointer;
	display: inline-block;
	font-family: $alt-font;
	font-size: 1em;
	letter-spacing: 0.1em;
	padding: 2px 5px;
	text-decoration: none;
	text-shadow: 1px 1px 1px #fff;
	vertical-align: top;
}

@mixin box_base {
	@include border-radius(5px);
	@include box-shadow(0 0 10px rgba(0,0,0,0.06));
	background: #fff;
	border: 1px solid #eee;
	margin: 5px 0;
	padding: 20px;
}





/* ===== Global Typographic / Element Styles ===== */

a {
   
   &:link, &:visited {
      color: $color2;
   }
   
   &:hover, &:active {
      color: $color2-dark;
   }
}

a.tag {
   background: #eee;
   border: 1px solid #ccc;
   border-radius: 5px;
   color: $color1;
   display: inline-block;
   font-size: 0.9em;
   line-height: 1em;
   text-decoration: none;
   padding: 3px 6px;
}

a.more {
   color: $color0;
   font-size: 0.9em;
   margin: 0 2px;
   text-decoration: none;
   text-transform: uppercase;
   
   &:hover, &:active {
      color: #000;
      text-decoration: underline;
   }
}

h1 {
   font-family: 'SommetSlabBlack', Arial, sans-serif;
   font-size: 2.5em;
   font-weight: normal;
   letter-spacing: 0.03em;
   line-height: 0.95em;
   text-align: center;
   text-transform: uppercase;
   
   text-shadow: 0 0 4px rgba(0, 0, 0, 0.9), 0 0 10px rgba(255, 255, 255, 0.2);
}

h2 {
   font-family: 'Asap', Arial, sans-serif;
   font-size: 1.6em;
   font-weight: 400;
   line-height: 1.3em;
   margin: 0 0 15px;
   
   em {
      color: $color2;
      font-weight: 400;
   }
}

h3 {
   font-family: 'Asap', Arial, sans-serif;
   font-size: 1.3em;
   font-weight: 700;
   line-height: 1.2em;
   margin: 0.5em 0;
}

h4 {
   font-family: 'Asap', Arial, sans-serif;
   font-size: 1.1em;
   font-weight: 700;
   line-height: 1.2em;
   margin: 0.25em 0;
}

p, ul, ol {
   font-family: 'Asap', Arial, sans-serif;
   font-size: 1.1em;
   font-weight: 400;
   line-height: 1.5em;
   margin: 0 0 0.5em;
   
   strong {
      font-weight: 700;
   }
   
   em {
   
   }
}

ul, ol {
   margin: 0;
   padding: 0;
}

fieldset {
   border-top: 1px solid #666;
   color: #ccc;
   margin-bottom: 10px;
   padding: 10px 0;
   &.none { display: none; }
}

label {
   // color: #ccc;
   display: block;
   line-height: 1.4em;
   margin-bottom: 5px;
   // padding-right: 150px;
   
   strong {
      color: #fff;
   }
}

input[type="radio"] {
   margin-left: 5px;
   
   &:first-child {
      margin-left: 0;
   }
}

input[type="submit"] {
   background: $color2;
   border: 1px solid #ccc;
   border-collapse: collapse;
   @include border-radius(5px);
   color: #fff;
   display: block;
   font-size: 1em;
   font-weight: 700;
   padding: 8px 14px;
   
   box-shadow:
      0 0 5px rgba(0, 0, 0, 0.9), 
      0 0 20px rgba(255, 255, 255, 0.1), 
      inset 0 -18px 0 rgba(0, 0, 0, 0.2), 
      inset -5px -5px 5px rgba(0, 0, 0, 0.5), 
      inset 5px 5px 5px rgba(255, 255, 255, 0.2);
   
   &:hover, &:active {
      background: $color2-dark;
   }
}


// global text-shadow for some elements

h2, label, footer, input[type="submit"] {
   text-shadow: 0 0 2px rgba(0, 0, 0, 1.0), 0 0 10px rgba(0, 0, 0, 0.5);
}





/* ===== Layout Styles & Specific Element Style Overrides ===== */

body {
   background: url('../img/wood2.png') 0 0 repeat #fff;
   color: $color0;
   font-size: 18px;
   line-height: 1.45em;
   width: $blueprint-container-size;
}

header {
   // @include column(1);
   float: left;
   width: 150px;
   padding-top: 20px;
}
   
section {
   // @include column(3, last);
   
   float: left;
   margin-bottom: 5px;
   // width: 768px;
   width: 618px;
   @include clearfix;
}

footer {
   // @include column(3, last);
   color: $color2;
   clear: both;
   float: left;
   font-size: 11px;
   letter-spacing: 0.06em;
   padding-left: 150px;
}
   
nav {
   // background: url('../img/pe-emblem1.png') 52px bottom no-repeat;
   // padding-bottom: 170px;
   text-align: center;
   
   button {
      background: url('../img/shiny-btn100-off.png') 0 0 no-repeat transparent;
      border: 0 none;
      border-collapse: collapse;
      display: block;
      font-family: 'SommetSlabBlack', Arial, sans-serif;
      font-size: 2em;
      margin: 0 auto 10px;
      height: 100px;
      width: 100px;
      text-shadow: 0 0 2px rgba(255, 255, 255, 0.9);
      
      &.on {
         background: url('../img/shiny-btn100-on.png') 0 0 no-repeat transparent;
      }
   }
}

#polls {
   
   article {
      background: rgba(0, 0, 0, 0.25);
      border: 2px solid #999;
      @include border-radius(20px);
      box-shadow: 
         0 0 5px rgba(0, 0, 0, 0.9), 
         0 0 20px rgba(255, 255, 255, 0.1), 
         inset 0 0 10px rgba(0, 0, 0, 0.9);
      margin: 20px 10px 20px 0;
      padding: 40px 50px;
      
      &:last-child {
         margin: 20px 10px 0 0;
      }
      
      .radio {
         background: url('../img/shiny-btn50-off.png') 0 0 no-repeat transparent;
         color: #333;
         curser: pointer;
         display: inline-block;
         font-weight: 700;
         height: 50px;
         line-height: 50px;
         text-align: center;
         text-decoration: none;
         width: 50px;
         
         &.on {
            background: url('../img/shiny-btn50-on.png') 0 0 no-repeat transparent;
            color: #000;
         }
         
         &:hover, &:active {
            color: #000;
         }
      }
   }
}





