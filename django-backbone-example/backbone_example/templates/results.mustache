<article>
    <h2>{{title}} Results</h2>
    <form>

        <fieldset class="results ratings">
            <h3>{{rating_container_label}}</h3>

            <span class="{{rating_label_1}}">
                <div class="rate_bar"><span><p>{{question1_avg}}/5</p></span><div class="rate_bar_value"></div></div>
                <strong class="none">{{question1_avg}}</strong> <p>{{rating_label_1}}</p>
            </span>

            <span class="{{rating_label_2}}">
                <div class="rate_bar"><span><p>{{question2_avg}}/5</p></span><div class="rate_bar_value"></div></div>
                <strong class="none">{{question2_avg}}</strong> <p>{{rating_label_2}}</p>
            </span>

            <span class="{{rating_label_3}}">
                <div class="rate_bar"><span><p>{{question3_avg}}/5</p></span><div class="rate_bar_value"></div></div>
                <strong class="none">{{question3_avg}}</strong> <p>{{rating_label_3}}</p>
            </span>

            <span class="{{rating_label_4}}">
                <div class="rate_bar"><span><p>{{question4_avg}}/5</p></span><div class="rate_bar_value"></div></div>
                <strong class="none">{{question4_avg}}</strong> <p>{{rating_label_4}}</p>
            </span>

            <fieldset>
                <h3>{{essay_question_label}}</h3>
                <div class="the_essays none">{{the_essays}}</div>
                <p class="random_essay"></p>
            </fieldset>
        </fieldset>

        <fieldset id="multi_graph" class="results multiple">
            <h3>{{multiple_container_label}}</h3>
            <div class="plotme" style="height:200px"></div>
            <fieldset>
                <div class="answers_col">
                    <span class="{{multiple_answer_1}}">
                        <p><strong>A</strong> {{multiple_answer_1}}</p>
                    </span>

                    <span class="{{multiple_answer_2}}">
                        <p><strong>B</strong> {{multiple_answer_2}}</p>
                    </span>

                    <span class="{{multiple_answer_3}}">
                        <p><strong>C</strong> {{multiple_answer_3}}</p>
                    </span>
                </div>
                <div class="    ">
                    <span class="{{multiple_answer_4}}">
                        <p><strong>D</strong> {{multiple_answer_4}}</p>
                    </span>

                    <span class="{{multiple_answer_5}}">
                        <p><strong>E</strong> {{multiple_answer_5}}</p>
                    </span>
                </div>
            </fieldset>
        </fieldset>

    </form>
</article>